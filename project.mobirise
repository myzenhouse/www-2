        {
          "_name": "tabs02",
          "_sourceTheme": "essencem5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"tabs02 essencem5\" group=\"List\" plugins=\"mbr-tabs\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <header>Tabs</header>\n        <select title=\"Count\" name=\"bootstrapTabs\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\">7</option>\n            <option value=\"8\">8</option>\n            <option value=\"9\" selected>9</option>\n            <option value=\"10\">10</option>\n        </select>\n        <input type=\"color\" title=\"Tabs Color\" value=\"#000000\" name=\"tabs\" selected>\n        <input type=\"color\" title=\"Tabs Active BG\" value=\"#a050ff\" name=\"active\" selected>\n        <input type=\"checkbox\" title=\"Tabs Number\" name=\"showNumber\" checked>\n        <input type=\"checkbox\" title=\"Tabs Icon\" name=\"showIcon\" checked>\n        <input type=\"checkbox\" title=\"Card Text\" name=\"showText\" checked>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/mbr-1920x1280.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#f6eee6\" selected>\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/essencem5/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#a050ff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12 col-lg-5\">\n                <div class=\"title-wrapper\">\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title, .tabs-wrap\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\"><b>Recorrido Virtual</b></h2>\n                </div>\n                <div class=\"tabs-wrap\">\n                    <ul class=\"nav nav-tabs\" role=\"tablist\">\n                        <li mbr-buttons mbr-theme-style=\"display-4\" class=\"nav-item first mbr-fonts-style\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link show active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs02-s_tab0\" aria-selected=\"true\" data-app-placeholder=\"Type Text\">\n                                <p class=\"card-number mbr-fonts-style\" data-app-selector=\".card-number\">\n                                    01\n                                </p>Bienvenida a tu Zen House<div class=\"icon-wrapper\">\n                                    <span class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div>\n                            </a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-4\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 1\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs02-s_tab1\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">\n                                <p class=\"card-number mbr-fonts-style\" data-app-selector=\".card-number\">\n                                    02\n                                </p>\n                                Intenciona tu espacio<div class=\"icon-wrapper\">\n                                    <span class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div>\n                            </a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-4\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 2\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs02-s_tab2\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">\n                                <p class=\"card-number mbr-fonts-style\" data-app-selector=\".card-number\">\n                                    03\n                                </p>Musicaliza tu espacio<br><div class=\"icon-wrapper\"><span class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div>\n                            </a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-4\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 3\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs02-s_tab3\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">\n                                <p class=\"card-number mbr-fonts-style\" data-app-selector=\".card-number\">\n                                    04\n                                </p>Restaurantes<div class=\"icon-wrapper\">\n                                    <span class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div>\n                            </a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-4\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 4\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs02-s_tab4\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">\n                                <p class=\"card-number mbr-fonts-style\" data-app-selector=\".card-number\">\n                                    05\n                                </p>Cocina<div class=\"icon-wrapper\">\n                                    <span class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div>\n                            </a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-4\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 5\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs02-s_tab5\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">\n                                <p class=\"card-number mbr-fonts-style\" data-app-selector=\".card-number\">\n                                    06\n                                </p>\n                                Descan-zen<div class=\"icon-wrapper\">\n                                    <span class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div>\n                            </a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-4\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 6\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs02-s_tab6\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">\n                                <p class=\"card-number mbr-fonts-style\" data-app-selector=\".card-number\">\n                                    07\n                                </p>\n                                Lee un buen libro<div class=\"icon-wrapper\">\n                                    <span class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div>\n                            </a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-4\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 7\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs02-s_tab7\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">\n                                <p class=\"card-number mbr-fonts-style\" data-app-selector=\".card-number\">\n                                    08\n                                </p>Descubre Experiencias<br><div class=\"icon-wrapper\"><span class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div>\n                            </a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-4\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 8\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs02-s_tab8\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">\n                                <p class=\"card-number mbr-fonts-style\" data-app-selector=\".card-number\">\n                                    09\n                                </p>Belleza &amp; Bienestar<br><div class=\"icon-wrapper\"><span class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div>\n                            </a></li>\n                        <li mbr-buttons mbr-theme-style=\"display-4\" class=\"nav-item\" mbr-if=\"bootstrapTabs > 9\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a class=\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs02-s_tab9\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">\n                                <p class=\"card-number mbr-fonts-style\" data-app-selector=\".card-number\">\n                                    10\n                                </p>\n                                Ll√©vate tu Zen House<div class=\"icon-wrapper\">\n                                    <span class=\"mobi-mbri mobi-mbri-up mbr-iconfont\"></span>\n                                </div>\n                            </a></li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-6\">\n                <div class=\"tab-content\">\n                    <div id=\"tab1\" class=\"tab-pane in active\" role=\"tabpanel\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                Aqu√≠ comienza tu viaje, acerca tu tel√©fono al primer c√≠rculo morado que se encuentra en la hoja de bienvenida.</p>\n                            <div class=\"image-wrapper\">\n                                <img src=\"@PROJECT_PATH@/assets/images/quetzal-bienvenida.webp\" alt=\"Mobirise\">\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab2\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 1\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                En zen house&nbsp;entendemos el poder de la intenci√≥n. Te invitamos a conectar con un prop√≥sito en esta estad√≠a, acerca tu tel√©fono a √©ste c√≠rculo morado para descubrir c√≥mo.</p>\n                            <div class=\"image-wrapper\">\n                                <img src=\"@PROJECT_PATH@/assets/images/quetzal-uno.webp\" alt=\"Mobirise\">\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab3\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 2\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                La m√∫sica tiene el poder de ambientar espacios para evocar ciertas emociones y estados mentales, encuentra nuestra selecci√≥n musical acercando tu celular a √©ste c√≠rculo morado.</p>\n                            <div class=\"image-wrapper\">\n                                <img src=\"@PROJECT_PATH@/assets/images/quetzal-tv.webp\" alt=\"Mobirise\">\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab4\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 3\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                Encuentra nuestra selecci√≥n especial de restaurantes.</p>\n                            <div class=\"image-wrapper\">\n                                <img src=\"@PROJECT_PATH@/assets/images/quetzal-comedor.webp\" alt=\"Mobirise\">\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab5\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 4\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">Encuentra nuestra selecci√≥n de deliciosas y saludables recetas</p>\n                            <div class=\"image-wrapper\">\n                                <img src=\"@PROJECT_PATH@/assets/images/quetzal-cocina.webp\" alt=\"Mobirise\">\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab6\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 5\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                Descubre herramientas para mejorar tu descanso.</p>\n                            <div class=\"image-wrapper\">\n                                <img src=\"@PROJECT_PATH@/assets/images/quetzal-rec-2.webp\" alt=\"Mobirise\">\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab7\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 6\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\"><b>\n                                Descubre nuestra selecci√≥n de libros recomendados.</b></p>\n                            <div class=\"image-wrapper\">\n                                <img src=\"@PROJECT_PATH@/assets/images/quetzal-rec-3.webp\" alt=\"Mobirise\">\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab8\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 7\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                Descubre nuestra selecci√≥n de experiencias para hacer en casa o al aire libre.</p>\n                            <div class=\"image-wrapper\">\n                                <img src=\"@PROJECT_PATH@/assets/images/quetzal%20-%20exterior-1256x942.jpg\" alt=\"Mobirise\">\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab9\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 8\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                Descubre nuestra selecci√≥n exclusiva de rituales de cuidado personal.</p>\n                            <div class=\"image-wrapper\">\n                                <img src=\"@PROJECT_PATH@/assets/images/quetzal-bano-3.webp\" alt=\"Mobirise\">\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"tab10\" class=\"tab-pane\" role=\"tabpanel\" mbr-if=\"bootstrapTabs > 9\">\n                        <div class=\"content-wrapper\">\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card-text\" mbr-if=\"showText\">\n                                Adquiere nuestro kit para convertir cualquier espacio en en zen house.</p>\n                            <div class=\"image-wrapper\">\n                                <img src=\"@PROJECT_PATH@/assets/images/zen%20house%20icosaedro-24.svg\" alt=\"Mobirise\">\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 1rem)",
              "padding-bottom": "(@paddingBottom * 1rem)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "overflow": "hidden",
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            ".mbr-fallback-image.disabled": {
              "display": "none"
            },
            ".mbr-fallback-image": {
              "display": "block",
              "background-size": "cover",
              "background-position": "center center",
              "width": "100%",
              "height": "100%",
              "position": "absolute",
              "top": "0",
              "& when (@bg-type = 'video')": {
                "background-image": "url(@fallBackImage)"
              }
            },
            "& when (@fullWidth)": {
              ".container-fluid": {
                "padding": "0 80px",
                "@media (max-width: 1200px)": {
                  "padding": "0 40px"
                },
                "@media (max-width: 992px)": {
                  "padding": "0 22px"
                }
              }
            },
            ".container": {
              "@media (max-width: 992px)": {
                "padding": "0 22px"
              }
            },
            ".row": {
              "justify-content": "space-between"
            },
            ".title-wrapper": {
              ".mbr-section-title": {
                "margin-bottom": "20px"
              }
            },
            ".tabs-wrap": {
              ".nav.nav-tabs": {
                "display": "inline-block",
                "width": "fit-content",
                "border": "none",
                "@media (max-width: 992px)": {
                  "display": "block",
                  "width": "100%"
                },
                ".nav-item": {
                  "padding": "0",
                  "border": "none",
                  "border-radius": "0",
                  "margin": "0",
                  ".nav-link": {
                    "display": "flex",
                    "align-items": "center",
                    "padding": "15px 80px 15px 20px",
                    "margin-bottom": "0",
                    "border": "none",
                    "border-bottom": "1px solid @tabs",
                    "color": "@tabs",
                    "background-color": "transparent",
                    "border-radius": "0 !important",
                    "transition": "all 0.3s ease-in-out",
                    "@media (max-width: 1200px)": {
                      "padding": "15px 20px"
                    },
                    "@media (max-width: 768px)": {
                      "padding": "15px 0"
                    },
                    ".card-number": {
                      "-webkit-text-stroke-width": "1px",
                      "-webkit-text-stroke-color": "@tabs",
                      "-webkit-text-fill-color": "transparent",
                      "transition": "all 0.3s ease-in-out",
                      "margin-right": "32px",
                      "margin-bottom": "0",
                      "min-width": "40px",
                      "@media (max-width: 1200px)": {
                        "margin-right": "16px"
                      },
                      "& when not (@showNumber)": {
                        "display": "none"
                      }
                    },
                    ".icon-wrapper": {
                      ".mbr-iconfont": {
                        "display": "inline-flex",
                        "font-size": "30px",
                        "transform": "rotate(45deg)",
                        "margin-left": "30px",
                        "color": "@active",
                        "opacity": "0",
                        "@media (max-width: 1200px)": {
                          "margin-left": "16px"
                        }
                      },
                      "& when not (@showIcon)": {
                        "display": "none"
                      }
                    }
                  },
                  "&:hover": {
                    ".icon-wrapper": {
                      ".mbr-iconfont": {
                        "opacity": "1"
                      }
                    }
                  },
                  ".nav-link.active": {
                    "color": "@active",
                    "border-bottom": "1px solid @active",
                    ".card-number": {
                      "-webkit-text-stroke-color": "@active"
                    }
                  }
                }
              }
            },
